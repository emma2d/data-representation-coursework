import requests
import json
from config import configkeyprivate as cfg


# Getting information from  private repo with an api key
url = f'https://api.github.com/repos/emma2d/aprivateone'
apiKey = cfg["privatekey"]

filename = "private.json"

response = requests.get(url, auth=('token',apiKey))
repoJSON = response.json()
print (response.json())

with open(filename, 'w') as fp:
    json.dump(repoJSON, fp, indent=4)

print (response.status_code)